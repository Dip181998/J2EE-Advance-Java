package com.jspiders.designpattern.operation;

import com.jspiders.designpattern.account.Account;

public class Withdraw {

	public void Withdraw(double amount) {
		System.out.println("withdraw amount" + amount + "available amount");
		Account.accountBalance -= amount;
		System.out.println("total availablebalance :"+Account.accountBalance);	
	}

}
